{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	}, 
	"rules": {
		"semi": "off",
		"object-curly-spacing": "off",
		"arrow-parens": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"no-tabs": "off",
		"indent": ["error", "tab"]
	},
	"extends": ["eslint:recommended", "google"]
}
